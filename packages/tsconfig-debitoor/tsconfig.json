{
	"compilerOptions": {
		"module": "commonjs",
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"target": "es2018",
		"sourceMap": true,
		"lib": [
			"es2018",
			"dom"
		],
		"noImplicitAny": true,
		"moduleResolution": "node",
		"noUnusedLocals": true,
		"types": [
			"node"
		]
	},
	"compileOnSave": true
}